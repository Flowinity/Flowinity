{
  "name": "tpu-server",
  "description": "TPUv2 Server",
  "version": "4.0.0",
  "private": true,
  "authors": [
    "Troplo <troplo@troplo.com>",
    "The Loose Goose"
  ],
  "scripts": {
    "start": "tsc --project tsconfig.app.json && node out/cluster.js",
    "serve": "THREADS=1 tsc-watch --project tsconfig.app.json --onSuccess 'node ./out/cluster.js'",
    "serve-cluster": "tsc-watch --project tsconfig.app.json --onSuccess 'node ./out/cluster.js'",
    "build": "tsc --project tsconfig.app.json",
    "format": "prettier --config ../.prettierrc.yaml --write \"app/**/*\"",
    "lint": "eslint -c .eslintrc.json \"./app/**/*.ts\" --format stylish",
    "test": "NODE_ENV=test yarn init:db && yarn test:no-db",
    "postinstall": "patch-package",
    "test:no-db": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --detectOpenHandles --forceExit --coverage",
    "init:db": "NODE_ENV=test ts-node ./app/testStart.ts"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "per-file": true,
    "include": [
      "app/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "app/index.ts",
      "app/server.ts",
      "app/app.ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "reporter": [
      "html",
      "text",
      "cobertura"
    ],
    "all": true,
    "cache": true,
    "sourceMap": true,
    "instrument": true
  },
  "_moduleAliases": {
    "@app": "out"
  },
